{
  "name": "nest-typescript-starter",
  "private": true,
  "version": "1.0.0",
  "description": "Nest TypeScript starter repository",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "format": "prettier --write \"src//*.ts\" \"test//*.ts\"",
    "start": "nest start",
    "build": "nest build",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "compliance-tests": "npm run compliance:lint",
    "compliance:lint": "eslint --ignore-path .gitignore .",
    "security:outdated": "npm outdated || true",
    "test": "jest -i",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "NODE_ENV=test jest --config ./test/jest-e2e.json",
    "compodoc": "npx compodoc -p tsconfig.doc.json"
  },
  "dependencies": {
    "@aws-amplify/auth": "^4.6.6",
    "@aws-sdk/cloudfront-signer": "^3.271.0",
    "@elastic/elasticsearch": "^8.6.0",
    "@nestjs-modules/mailer": "^1.8.1",
    "@nestjs/common": "^9.0.11",
    "@nestjs/config": "^2.3.1",
    "@nestjs/core": "^9.0.11",
    "@nestjs/elasticsearch": "^9.0.0",
    "@nestjs/jwt": "^9.0.0",
    "@nestjs/mongoose": "^9.2.0",
    "@nestjs/passport": "^9.0.0",
    "@nestjs/platform-express": "^9.0.11",
    "@nestjs/swagger": "^6.1.2",
    "@sentry/node": "^7.12.1",
    "@sentry/tracing": "^7.12.1",
    "ajv": "^8.11.0",
    "amazon-cognito-identity-js": "^5.2.14",
    "aws-cloudfront-sign": "^2.2.0",
    "aws-jwt-verify": "^3.1.0",
    "aws-sdk": "^2.1216.0",
    "bcrypt": "5.0.1",
    "body-parser": "^1.20.1",
    "cache-manager": "^4.0.0",
    "cache-manager-redis-store": "^2.0.0",
    "cjs": "^0.0.11",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "elasticsearch": "^16.7.3",
    "express": "^4.18.2",
    "firebase-admin": "^11.0.1",
    "fs": "^0.0.1-security",
    "handlebars": "^4.7.7",
    "helmet": "^6.0.0",
    "i18n": "^0.15.0",
    "jwk-to-pem": "^2.0.5",
    "jwks-rsa": "^2.1.4",
    "loader": "^2.1.1",
    "mongoose": "^6.6.1",
    "multer": "^1.4.5-lts.1",
    "multer-s3": "^3.0.1",
    "mustache": "^4.2.0",
    "nestjs-telegraf": "^2.6.2",
    "nodemailer": "^6.9.1",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "python": "^0.0.4",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.5.6",
    "sharp": "0.31.0",
    "source-map-support": "^0.5.21",
    "swagger-ui-express": "^4.5.0",
    "telegraf": "^4.11.2",
    "uuidv4": "^6.2.13"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.1.3",
    "@nestjs/schematics": "^9.0.3",
    "@nestjs/testing": "^9.0.11",
    "@types/bcrypt": "^5.0.0",
    "@types/cache-manager": "^4.0.2",
    "@types/cache-manager-redis-store": "^2.0.1",
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^4.17.14",
    "@types/jest": "^29.0.2",
    "@types/multer": "^1.4.7",
    "@types/node": "^18.7.18",
    "@types/nodemailer": "^6.4.6",
    "@types/passport-jwt": "^3.0.6",
    "@types/passport-local": "^1.0.34",
    "@types/sharp": "^0.30.5",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.37.0",
    "@typescript-eslint/parser": "^5.37.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.1",
    "jest": "^29.0.3",
    "mongodb-memory-server": "^8.9.2",
    "prettier": "^2.7.1",
    "serve": "^14.0.1",
    "source-map-support": "^0.5.21",
    "supertest": "^6.2.4",
    "ts-jest": "^29.0.1",
    "ts-loader": "^9.3.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.0",
    "typescript": "^4.8.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "main": ".eslintrc.js",
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git@gitlab.sigmasoftwares.com:jupap/jupap-backend.git"
  },
  "author": ""
}
